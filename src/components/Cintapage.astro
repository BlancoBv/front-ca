
---
interface Props {
  title?: string;
  url?  :any;
}
const { title,url } = Astro.props;
---
      <div class="flex sm:px-28  px-10 w-full h-20 items-center justify-between bg-gray-200">
        <h1 class="font-semibold text-3xl">{title}</h1>
        <ul class="hidden sm:flex">
          <li class="hover:text-blue-400 transition-colors duration-300 pr-1">
            <a href="#">Inicio</a>
          </li>
          <li>{url}</li>
        </ul>
      </div>


