---
import type { GetStaticPaths } from "astro";
import Layout, { urlMain } from "../../layouts/Layout.astro";
import MiembroPage from "../../components/MiembroPage";
import members from "../../../public/configs/members.json";
import Tablespersonal from "../../components/Tablespersonal.astro";

export const getStaticPaths = (async () => {
  return members.map((el: { id: number }) => ({
    params: { miembro: String(el.id) },
  }));
}) satisfies GetStaticPaths;
const { miembro } = Astro.params;
---

<Layout title=`Miembro ${miembro}`>
  <MiembroPage id={miembro} client:visible />
  <Tablespersonal id={miembro} client:visible/>
</Layout>
